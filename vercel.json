{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/public/.*",
      "use": "@vercel/static" 
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods":[
        "GET",
        "POST",
        "PATCH",
        "DELETE"
      ]
    }
  ]
}
